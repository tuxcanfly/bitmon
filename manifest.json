{
  "name": "Bitmon",
  "description": "Monitor your bitcoin wallet",
  "version": "1.0",
  "permissions": [
    "tabs", "http://127.0.0.1/*"
  ],
  "browser_action": {
      "default_title": "Show wallet details",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
